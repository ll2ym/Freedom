# App Configuration
APP_NAME=FreedomServer
APP_ENV=development
APP_PORT=8000

# Database
DB_URL=sqlite:///./freedom.db

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=1440

# Encryption
ENCRYPTION_KEY=your-base64-encoded-256-bit-encryption-key

# WebRTC ICE Servers
ICE_SERVERS=[{"urls": "stun:stun.l.google.com:19302"}]

# Rate Limiting
RATE_LIMIT_GLOBAL=100/minute
RATE_LIMIT_LOGIN=5/minute
RATE_LIMIT_WS=50/minute

# Security
INVITE_ONLY=true